#include <stdio.h>
#include <stdlib.h>
void main()
{
char k[100],st[100],a[100],b[100];
int v,i=0,p;
printf("enter ip address:\n");
scanf("%d",&st);
while(st[i]!='.')
{
k[i]=st[i];
i++;
}
v=atoi(k);
printf("%d",v);
if(v>=0 && v<128)
{
printf("class A\n");
p=1;
}
if(v>=128 && v<192)
{
printf("class B\n");
p=2;
}
if(v>=192 && v<224)
{
printf("class C\n");
p=3;
}
int j=0,r=0,l=0;
for(i=0;st[i]!='\0';i++)
{
if(st[i]=='.')
  r++;
if(r>=p)
{
b[j]=st[i];
j++;
}
else
{
a[l]=st[i];
l++;
}
}
b[j]='\0';
printf("net id is %s, host id is %s\n",a,b);
}
